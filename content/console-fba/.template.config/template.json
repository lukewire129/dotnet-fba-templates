{
  "$schema": "http://json.schemastore.org/template",
  "author": "rkttu",
  "classifications": [ "Common", "Console", "File-based App" ],
  "identity": "console-fba",
  "name": "Console (File-based App)",
  "shortName": "console-fba",
  "sourceName": "Program",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project (e.g., net10.0, net9.0, net8.0). Leave empty to use the latest available.",
      "datatype": "string",
      "defaultValue": "",
      "replaces": "NET_TFM_PARAM"
    },
    "EnableAot": {
      "type": "parameter",
      "description": "Enable AOT compilation",
      "datatype": "bool",
      "defaultValue": "false"
    }
  }
}