{
    "$schema": "http://json.schemastore.org/template",
    "author": "rkttu",
    "classifications": [ "Web", "MVC", "FBA" ],
    "identity": "mvc-fba",
    "name": "ASP.NET Core MVC (File-based App)",
    "shortName": "mvc-fba",
    "sourceName": "Program",
    "preferNameDirectory": true,
    "tags": {
