{
    "$schema": "http://json.schemastore.org/template",
    "author": "rkttu",
    "classifications": [ "Common", "WebAssembly", "File-based App" ],
    "identity": "wasm-fba",
    "name": "WebAssembly (File-based App)",
    "shortName": "wasm-fba",
    "sourceName": "Program",
    "preferNameDirectory": true,
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "symbols": {
        "Framework": {
            "type": "parameter",
            "description": "The target framework for the project (e.g., net10.0-browser, net9.0-browser). Leave empty to use the latest available.",
            "datatype": "string",
            "defaultValue": "",
            "replaces": "NET_TFM_PARAM"
        },
        "EnableAot": {
            "type": "parameter",
            "description": "Enable AOT compilation (Note: WebAssembly cannot use NativeAOT)",
            "datatype": "bool",
            "defaultValue": "false"
        }
    }
}